networks:
  ipasnetwork:
    external:
      name: ipasnetwork
services:
  registrationservice.api:
    build:
      context: C:\Users\dreck\source\repos\RegistrationService.Data\SimpleRegistrationService
      dockerfile: RegistrationService.API/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: RegistrationService.API
    container_name: RegistrationService.API
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    hostname: registrationservice.api
    image: registrationserviceapi
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "RegistrationService.API.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      ipasnetwork: {}
    ports:
    - target: 80
    - published: 30001
      target: 30001
    - published: 30021
      target: 30021
    tty: true
    volumes:
    - C:\Users\dreck\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\dreck\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\dreck\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
version: '3.4'